    "@types/node-fetch": "^2",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
